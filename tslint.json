{
  "rules": {
    "no-duplicate-variable": true,
    "class-name": true,
    "semicolon": [
      true,
      "always"
    ],
    "eofline": true,
    "no-trailing-whitespace": true,
    "no-var-keyword": true,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "trailing-comma": [
      true
    ],
    "triple-equals": [
      true
    ],
    "adjacent-overload-signatures": true,
    "restrict-plus-operands": true
  }
}
